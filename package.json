{
  "name": "quality-metrics-dashboard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n api,vite -c blue,green \"tsx src/api/server.ts\" \"vite\"",
    "dev:api": "tsx src/api/server.ts",
    "dev:vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:scripts": "vitest run --config /dev/null --dir scripts/__tests__",
    "typecheck": "tsc --noEmit",
    "populate": "tsx scripts/populate-dashboard.ts",
    "sync": "tsx scripts/sync-to-kv.ts",
    "deploy:worker": "wrangler deploy",
    "dev:worker": "wrangler dev"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "@tanstack/react-query": "^5.0.0",
    "hono": "^4.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "wouter": "^3.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "jsdom": "^28.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.0.0",
    "@cloudflare/workers-types": "^4.0.0"
  }
}
